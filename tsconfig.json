{
	"include": [
		"src"
	],
	"compilerOptions": {
		"incremental": true,
		"target": "ES2022",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"lib": [
			"ESNext",
		],
		"module": "CommonJS",
		"types": [],
		"resolveJsonModule": true,
		"sourceMap": true,
		"outDir": "./dist",
		"importHelpers": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": false,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"skipLibCheck": true,
	}
}